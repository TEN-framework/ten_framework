gtest_output_libs = []

if (is_win) {
  gtest_output_libs +=
      [ "${root_out_dir}/obj/third_party/googletest/gtest.lib" ]
} else if (is_mac) {
  gtest_output_libs +=
      [ "${root_out_dir}/obj/third_party/googletest/libgtest.a" ]
} else if (is_linux) {
  gtest_output_libs +=
      [ "${root_out_dir}/obj/third_party/googletest/libgtest.a" ]
}
