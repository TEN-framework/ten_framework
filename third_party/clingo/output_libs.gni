import("//build/ten_runtime/options.gni")

clingo_output_libs = []

if (is_linux || is_mac) {
  clingo_output_libs +=
      [ "${root_gen_dir}/cmake/clingo/install/lib/libclingo.a" ]
} else if (is_win) {
  clingo_output_libs +=
      [ "${root_gen_dir}/cmake/clingo/install/bin/clingo.dll" ]
}
