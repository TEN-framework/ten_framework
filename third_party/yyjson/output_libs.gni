yyjson_output_libs = []

if (is_win) {
  yyjson_output_libs +=
      [ "${root_gen_dir}/cmake/yyjson/install/lib/yyjson.lib" ]
} else if (is_mac) {
  yyjson_output_libs +=
      [ "${root_gen_dir}/cmake/yyjson/install/lib/libyyjson.a" ]
} else if (is_linux) {
  yyjson_output_libs +=
      [ "${root_gen_dir}/cmake/yyjson/install/lib/libyyjson.a" ]
}
