import("//build/ten_runtime/options.gni")

group("nodejs") {
  deps = [
    "call_api_after_closing_nodejs",
    "cpp_app_nodejs",
    "get_set_property_nodejs",
    "http_server_nodejs",
    "mix_python_ext_nodejs",
    "send_audio_frame_nodejs",
    "send_cmd_nodejs",
    "send_data_nodejs",
    "send_video_frame_nodejs",
    "websocket_server_nodejs",
