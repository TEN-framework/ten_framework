import("//build/ten_utils/utils.gni")

ten_utils_glob("stream") {
  deps = [ "//third_party/libuv:uv_a" ]
  if (!is_win) {
    cflags = [ "-Wno-incompatible-pointer-types" ]
  } else {
    cflags = [
      "/wd4113",
      "/wd4133",
    ]
  }
}
