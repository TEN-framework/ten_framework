import("//build/ten_utils/utils.gni")

ten_utils_glob("posix") {
  deps = [ "dwarf_internal" ]

  if (is_linux) {
    deps += [ "linux" ]
  } else if (is_mac) {
    deps += [ "darwin" ]
  }
}
