{
  "_ten": {
    "predefined_graphs": [
      {
        "name": "test_graph",
        "connections": [
          {
            "app": "http://example.com:8000",
            "extension": "extension_1",
            "cmd": [
              {
                "dest": [
                  {
                    "app": "http://example.com:8000",
                    "extension": "extension_2"
                  }
                ],
                "name": "existing_cmd"
              },
              {
                "name": "cmd_with_conversion",
                "dest": [
                  {
                    "app": "http://example.com:8000",
                    "extension": "extension_2",
                    "msg_conversion": {
                      "type": "per_property",
                      "keep_original": true,
                      "rules": [
                        {
                          "path": "mapped_param",
                          "conversion_mode": "from_original",
                          "original_path": "original_param"
                        },
                        {
                          "path": "fixed_param",
                          "conversion_mode": "fixed_value",
                          "value": 42
                        }
                      ],
                      "result": {
                        "type": "per_property",
                        "rules": [
                          {
                            "path": "mapped_result",
                            "conversion_mode": "from_original",
                            "original_path": "original_result"
                          }
                        ]
                      }
                    }
                  }
                ]
              }
            ]
          }
        ]
      }
    ]
  }
}