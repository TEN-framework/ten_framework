{
  "_ten": {
    "uri": "http://example.com:8000",
    "predefined_graphs": [
      {
        "name": "default_with_app_uri",
        "auto_start": true,
        "nodes": [
          {
            "app": "http://example.com:8000",
            "extension_group": "extension_group_1",
            "type": "extension",
            "addon": "extension_1",
            "name": "extension_1"
          },
          {
            "app": "http://example.com:8000",
            "extension_group": "extension_group_1",
            "type": "extension",
            "addon": "extension_2",
            "name": "extension_2",
            "property": {
              "a": 1
            }
          },
          {
            "app": "http://example.com:8000",
            "extension_group": "extension_group_1",
            "type": "extension",
            "addon": "extension_3",
            "name": "extension_3"
          }
        ],
        "connections": [
          {
            "app": "http://example.com:8000",
            "extension": "extension_1",
            "cmd": [
              {
                "name": "hello_world",
                "dest": [
                  {
                    "app": "http://example.com:8000",
                    "extension": "extension_2"
                  }
                ]
              },
              {
                "name": "test_cmd_for_update",
                "dest": [
                  {
                    "app": "http://example.com:8000",
                    "extension": "extension_2",
                    "msg_conversion": {
                      "type": "per_property",
                      "rules": [
                        {
                          "path": "request_property",
                          "conversion_mode": "fixed_value",
                          "value": "request_value"
                        },
                        {
                          "path": "new_copied_property",
                          "conversion_mode": "from_original",
                          "original_path": "original_source"
                        }
                      ],
                      "keep_original": true,
                      "result": {
                        "type": "per_property",
                        "rules": [
                          {
                            "path": "response_property",
                            "conversion_mode": "from_original",
                            "original_path": "original_response"
                          }
                        ]
                      }
                    }
                  }
                ]
              }
            ]
          }
        ]
      }
    ]
  }
}