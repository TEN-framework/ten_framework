import("//build/ten_runtime/glob.gni")

glob("binding") {
  file_list = all_native_files

  deps = []

  if (ten_enable_go_binding) {
    deps += [ "go" ]
  }

  if (ten_enable_python_binding) {
    deps += [ "python" ]
  }
}
