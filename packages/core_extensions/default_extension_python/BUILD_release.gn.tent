import("//build/feature/ten_package.gni")

ten_package("{{package_name}}") {
  package_kind = "extension"

  resources = [
    "__init__.py",
    "addon.py",
    "extension.py",
    "log.py",
    "manifest.json",
    "property.json",
  ]
}
