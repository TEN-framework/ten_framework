import("//build/ten_runtime/options.gni")

group("core_addon_loaders") {
  deps = []

  if (ten_enable_python_binding) {
    deps += [ "python_addon_loader" ]
  }

  if (ten_enable_nodejs_binding) {
    deps += [ "nodejs_addon_loader" ]
  }
}
